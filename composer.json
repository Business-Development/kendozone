{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.*",
    "laravelcollective/html": "5.2.*",
    "guzzlehttp/guzzle": "~6.0",
    "webpatser/laravel-countries": "dev-master",
    "laravel/socialite": "2.*",
    "torann/geoip": "0.2.*@dev",
    "maatwebsite/excel": "2.*",
    "barryvdh/laravel-debugbar": "2.*",
    "barryvdh/laravel-ide-helper": "2.*",
    "davejamesmiller/laravel-breadcrumbs": "3.*",
    "cviebrock/eloquent-sluggable": "3.*",
    "spatie/laravel-backup": "3.*",
    "spatie/laravel-link-checker": "1.*",
    "artesaos/seotools": "0.8.*",
    "proengsoft/laravel-jsvalidation": "1.*",
    "thomaswelton/laravel-gravatar": "1.*",
    "intervention/image": "2.*",
    "owen-it/laravel-auditing": "2.*",
    "barryvdh/laravel-translation-manager": "0.2.x-dev",
    "fzaninotto/faker": "1.*",
    "graham-campbell/htmlmin": "4.*",
    "codeception/codeception": "*",
    "sentry/sentry-laravel": "0.3.*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "4.*",
    "symfony/dom-crawler": "^3.0",
    "symfony/css-selector": "~3.0"

  },
  "autoload": {
    "files": [
      "app/Http/helpers.php"
    ],
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/functional/TestCase.php"
    ],
    "psr-4": {
      "Kendozone\\": "app/Kendozone"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize",
      "php artisan vendor:publish --provider=\"Proengsoft\\JsValidation\\JsValidationServiceProvider\" --tag=public --force"

    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
